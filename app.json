{
  "expo": {
    "name": "Star Wars - Wiki",
    "slug": "star-wars-wiki",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#161616"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#161616"
      }
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "description": "Star Wars - Wiki is a app to show films and characters of Star Wars Universe. This app was built em React Native with Expo.",
    "githubUrl": "https://github.com/ajvideira/sevenapps-starwarswiki"
  }
}
